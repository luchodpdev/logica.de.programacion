Algoritmo EjercicioOperadoresAritmeticos2
	Imprimir "Proporciona el primer valor: "
	leer valor1
	Imprimir "Proporciona el segundo valor: "
	leer valor2
	Imprimir "Suma: ", valor1+valor2
	Imprimir "Resta: ", valor1-valor2
	Imprimir "Multipica: ", valor1*valor2
	Imprimir "Divide: ", valor1/valor2
	Imprimir "Potencia: ", valor1^valor2
	Imprimir "MOD: ", valor1 MOD valor2
	
	
FinAlgoritmo
