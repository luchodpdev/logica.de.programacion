Algoritmo OperadoresAritmeticos
	//Operadores Aritmeticos
	//Suma +
	a = 3 + 4
	Imprimir "Suma: " a
	//Resta -
	b = 6 - 2
	Imprimir "Resta: " b
	//Multiplicacion
	c = a * 2
	Imprimir "Multiplicacion: ", c
	//Division
	d = b / c
	Imprimir "Division: ", d
	//Potencia ^
	e = a ^ 2
	Imprimir "Potencia: ", e
	//Modulo (residuo division)
	f = 28 % 5
	Imprimir "Modulo: ", f
	
	
FinAlgoritmo
